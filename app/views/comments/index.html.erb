<h4>Comments</h4>


<table class="striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
          <th colspan="3"></th>
    </tr>
  </thead>

 <tbody>
   <tbody>
     <tr>
  <% @comments.each do |comment| %>
